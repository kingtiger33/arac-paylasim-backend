{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cleanup.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cleanup",
      "dest": "/api/cleanup.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
